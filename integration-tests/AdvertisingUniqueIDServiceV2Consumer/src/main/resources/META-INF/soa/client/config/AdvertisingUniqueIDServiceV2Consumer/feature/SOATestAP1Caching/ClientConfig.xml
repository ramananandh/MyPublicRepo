<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<client-config-list xmlns="http://www.ebayopensource.org/turmeric/common/config">
    <client-config service-name="{http://www.ebayopensource.org/turmeric/v1/services}SOATestAP1Caching">
        <service-interface-class-name>org.ebayopensource.turmeric.services.soatestap1caching.SOATestAP1Caching</service-interface-class-name>
        <service-location>http://localhost:8080/ws/spf</service-location>
        <client-instance-config>
            <invocation-options>
                <preferred-transport name="HTTP11"/>
                <request-data-binding>XML</request-data-binding>
                <response-data-binding>XML</response-data-binding>
                <invocation-use-case>SOATestAP1CachingClient</invocation-use-case>
            </invocation-options>
             <cache-policy>
					<cache-provider-class-name>org.ebayopensource.turmeric.services.soatestap1caching.AdvertisingUniqueIDServiceV2Consumer.SOATestCacheProvider</cache-provider-class-name>
					<disable-cache-on-local>true</disable-cache-on-local>
			</cache-policy>
        </client-instance-config>
    </client-config>
</client-config-list>
