		</xs:documentation>
		<xs:appinfo/>
	</xs:annotation>
	<xs:complexContent>
		<xs:extension base="tns:BaseServiceResponse">
			<xs:sequence>
				<xs:element minOccurs="0" name="profileID" type="xs:string">
					<xs:annotation>
						<xs:documentation>
