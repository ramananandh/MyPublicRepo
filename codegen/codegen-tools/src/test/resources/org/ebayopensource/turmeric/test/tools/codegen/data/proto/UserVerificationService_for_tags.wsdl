						<xs:sequence>
							<xs:element maxOccurs="unbounded" minOccurs="0" name="verificationEntityMetadata" type="tns:VerificationEntityMetadata"/>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
