      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="getTrustStore">
      <soap:operation soapAction="getTrustStore"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="EsamsBridgeService">
    <wsdl:port binding="tns:soapNameService" name="soapNameService">
      <soap:address location="http://localhost:8080/ws/spf"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>