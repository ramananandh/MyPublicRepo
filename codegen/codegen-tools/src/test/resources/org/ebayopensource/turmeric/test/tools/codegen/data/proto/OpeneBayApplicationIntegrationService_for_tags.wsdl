		</xs:documentation>
	</xs:annotation>
	<xs:complexContent>
		<xs:extension base="tns:OpeneBayBaseSecurityTokenType">
			<xs:attribute name="tokenref" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Reserved for future use.
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:extension>
	</xs:complexContent>
</xs:complexType>


<xs:complexType name="OpeneBaySecurityTokenType">
	<xs:annotation>
		<xs:documentation>
			[???]
		</xs:documentation>
	</xs:annotation>
	<xs:complexContent>
		<xs:extension base="tns:OpeneBayBaseSecurityTokenType">
			<xs:sequence>
				<xs:element name="tokenValue" type="xs:string">
					<xs:annotation>
						<xs:documentation>
							The application's EIDP token, received in response to the Login request
