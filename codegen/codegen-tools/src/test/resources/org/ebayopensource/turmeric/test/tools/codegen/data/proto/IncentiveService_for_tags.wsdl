					<xs:documentation>
						This is a request container for the addCampaign
						operation
					</xs:documentation>
				</xs:annotation>
				<xs:complexContent>
					<xs:extension base="tns:BaseIncentiveServiceRequest">
						<!-- The default value for both maxOccurs and minOccurs is "1", so if these attributes are not specified the element must be present exactly once.-->
						<xs:sequence>
							<xs:element name="campaign" type="tns:Campaign"/>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
