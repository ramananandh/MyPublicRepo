							<xs:element maxOccurs="1" minOccurs="0" name="securityDomain" type="tns:SecurityDomain">
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="SubjectKey">
				<xs:annotation>
					<xs:documentation>
