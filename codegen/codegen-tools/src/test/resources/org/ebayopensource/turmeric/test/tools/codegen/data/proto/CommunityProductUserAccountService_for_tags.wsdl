									</xs:documentation>
									<xs:appinfo>
										<CallInfo>
											<CallName>
												GetProductDetails
											</CallName>
											<Returned>
												Conditionally
											</Returned>
										</CallInfo>
									</xs:appinfo>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
			</xs:complexType>
			<xs:complexType name="UserAccountDetailsForProduct">
				<xs:annotation>
					<xs:documentation>
