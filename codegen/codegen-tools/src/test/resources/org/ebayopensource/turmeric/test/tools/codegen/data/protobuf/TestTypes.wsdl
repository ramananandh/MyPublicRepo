    <xs:sequence>
    <xs:element name="sometype">
				    <xs:complexType>
				    
				    <xs:sequence>
				    <xs:element name="test" type="xs:string"/>
				    </xs:sequence>
				    </xs:complexType>
    </xs:element>
    
    </xs:sequence>
    </xs:extension>
  </xs:complexContent>
