    <part element="tns:getVersionResponse" name="messageParameters">
    </part>
  </message>
  
  <portType name="FundStatusServicePort">
    <operation name="addOrderDecision">
		<wsdl:documentation>Adds data pertaining to the hold or release decision of one or more orders.</wsdl:documentation>
      		<input message="tns:addOrderDecisionRequest" name="addOrderDecisionRequest"/>
      		<output message="tns:addOrderDecisionResponse" name="addOrderDecisionResponse"/>
    </operation>
    <operation name="getLineItemDecision">
		<wsdl:documentation>
			Retrieves data pertaining to the hold or release decision of one or more line items given
			transaction ids, item ids and optionally estimated release dates
		</wsdl:documentation>
     	<input message="tns:getLineItemDecisionRequest" name="getLineItemDecisionRequest"/>
     	<output message="tns:getLineItemDecisionResponse" name="getLineItemDecisionResponse"/>
    </operation>
    <operation name="getOrderDecision">
		<wsdl:documentation>
			Retrieves data pertaining to the hold or release decision of one or more orders given
			order ids and optionally estimated release dates
		</wsdl:documentation>
      	<input message="tns:getOrderDecisionRequest" name="getOrderDecisionRequest"/>
      	<output message="tns:getOrderDecisionResponse" name="getOrderDecisionResponse"/>
    </operation>
    <operation name="getVersion">
		<wsdl:documentation>Returns the current version of the service.</wsdl:documentation>
      	<input message="tns:getVersionRequest" name="getVersionRequest"/>
      	<output message="tns:getVersionResponse" name="getVersionResponse"/>
    </operation>
  </portType>
  
  <binding name="FundStatusServiceSOAPBinding" type="tns:FundStatusServicePort">
    <soap12:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <operation name="addOrderDecision">
      <soap12:operation soapAction="urn:addOrderDecision" style="document"/>
