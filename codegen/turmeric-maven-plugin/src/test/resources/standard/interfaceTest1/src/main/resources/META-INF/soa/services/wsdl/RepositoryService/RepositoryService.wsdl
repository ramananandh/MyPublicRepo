                            <xs:element name="library" type="tns:Library">
                                <xs:annotation>
                                    <xs:documentation>
                                        LibraryId or libraryName need be provided
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>                           
                        </xs:sequence>
